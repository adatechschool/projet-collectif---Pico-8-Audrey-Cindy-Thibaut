--mort du personnage

function dead_water()
 if fget(mget(flr(p.x/8),flr(p.y/8)),0) then 
  gravity=1
 elseif p.y==56 then 
  gravity=0 
 end
end

function dead_hole()
 if p.y>80 then
  sfx(2)
  p.x=13
  p.y=56
 end
end


 
-->8
--cle et interaction

function check_flag(flag,x,y)
 local sprite=mget(flr(x/8),flr(y/8))
 return fget(sprite,flag)
end

function next_tile(x,y)
 sprite=mget(flr(x/8),flr(y/8))
 mset(flr(x/8),flr(y/8),sprite+1) 
end

function pick_up_key(x,y)
 next_tile(x,y)
 p.key+=1
 sfx(1) 
end

function interact(x,y)
 if check_flag(2,x,y) then
  pick_up_key(x,y)
 end
 if check_flag(7,p.x,p.y) and anne_text==true then
  text_end()
  anne_text=false
 end
end


-->8
--porte du chateau

function unlock()
 if check_flag(5,p.x,p.y) and p.key==1 then
  if open==false then 
   next_tile(74*8,7*8)
   next_tile(74*8,6*8)
   next_tile(74*8,5*8)
   next_tile(73*8,8*8)
   next_tile(72*8,8*8)
   open=true
  end
 end
end

function door_castle()
	if p.x>584 then
	 p.y=24 
	end	
end

