--creation de message

function init_msg()
	messsages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

function draw_msg()
 if messages[1] then
  local y=(camy+80)
  local x=(camx)
  local msgx=(x+7)
  local msgy=(y+1)
  --titre
  rectfill(msgx-3,msgy+5,msgx+25,msgy-2,8)
  print(msg_title,x+7,y+1,7)
  --message
  --(solution par alice)
  rectfill(msgx-3,msgy+6,msgx+115,msgy+37,14)
  rect(msgx-3,msgy+6,msgx+115,msgy+37,8)
  print(messages[1],msgx,y+10,7)
 end
end

function update_msg()
 if (btnp(❎)) then
  deli(messages,1)
 end
end 


