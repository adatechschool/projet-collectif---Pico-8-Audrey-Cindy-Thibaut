function _init()
  p={x=13,y=56,f=false,sprite=20,key=0}
  gravity=1
  anne_text=true
  init_msg()
  depart_msg()
end

function _update()
 if not messages[1] then
 move_p()
 end
 jumping()
 update_camera()
 water()
 p_gravity()
 dead_hole()
 door_castle()
 update_msg()
end

function _draw()
 map(0,0,0,0)
 spr(p.sprite,p.x,p.y,1,1,p.f)
 draw_msg()
 transp()
end
-->8

function update_camera()
local map_width=(77*8)
local map_height=(16*8)
camx=mid(0,p.x-55,map_width-128)
camy=mid(0,p.y-55,map_height)
-- if p.x<552 then
  camera(camx,camy)
 --end
  
 end
 
function move_p()
 
  if (btn(➡️) and p.x<600) 
   then p.x+=1
        p.f=false
  end
  if (btn(⬅️) and p.x>0)
   then p.x-=1
        p.f=true 
  end
  interact(p.x,p.y)
  unlock()
end

  
function jumping()
 if (btnp(⬆️)) and p.y==56 then 
  	p.y-=17
  	gravity=0.5
 end 
end

-->8
function water()
  if fget(mget(flr(p.x/8),flr(p.y/8)),0)  
  then gravity=1
  elseif p.y==56
  then gravity=0 end
end

function p_gravity()
  if gravity>0
  then p.y+=gravity end
end

function dead_hole()
 if p.y>80 then
    sfx(2)
    p.x=13
    p.y=56
 end
  
end


 
-->8

function check_flag(flag,x,y)
 local sprite=mget(flr(x/8),flr(y/8))
 return fget(sprite,flag)
end

function next_tile(x,y)
 sprite=mget(flr(x/8),flr(y/8))
 mset(flr(x/8),flr(y/8),sprite+1) 
end

function pick_up_key(x,y)
 next_tile(x,y)
 p.key+=1
 sfx(1) 
end

function interact(x,y)
  if check_flag(2,x,y) then
  pick_up_key(x,y)
  end
  if check_flag(7,p.x,p.y) and anne_text==true
   then
   text_end()
   anne_text=false
   end
end


-->8
open=false

function unlock()
  if check_flag(5,p.x,p.y) and 
  p.key==1 then
 if open==false 
  then next_tile(74*8,7*8)
       next_tile(74*8,6*8)
       next_tile(74*8,5*8)
       next_tile(73*8,8*8)
       next_tile(72*8,8*8)
       open=true
       end
  end
end

function door_castle()
	if p.x>584
		then p.y=24 end	
end

-->8
-- dialogue

function init_msg()
	messsages={}
end

function create_msg(name,...)
	msg_title=name
	messages={...}
end

function draw_msg()
 if messages[1] then
  local y=(camy+80)
  local x=(camx)
  local msgx=(x+7)
  local msgy=(y+1)
  --titre
  --rectfill(x+6,y,6+#msg_title*4,y+10,8)
  rectfill(msgx-3,msgy+5,msgx+25,msgy-2,8)
  print(msg_title,x+7,y+1,7)
  --message
 -- rectfill(x+3,y+6,125,y+37,14)
  rectfill(msgx-3,msgy+6,msgx+115,msgy+37,14)
  rect(msgx-3,msgy+6,msgx+115,msgy+37,8)
  print(messages[1],msgx,y+10,7)
 end
end

function update_msg()
 if (btnp(❎)) then
  deli(messages,1)
 end
end 


