function depart_msg()
 create_msg("sandra",
  [[
  bonjour ada !
  que penses tu de nos
  nouveaux locaux ?
  impressionnants,non ?
  ]],
  [[
  j'aimerais pas etre 
  celui qui lavera
  toutes ces vitres...
  ]],
  [[
  au fait, j'ai croise anne,
  elle t'attend a son
  chateau !
  va la rejoindre !
  ]], 
  [[
  mais attention ! 
  la cle a ete perdue en 
  chemin, il te la faudra
  pour acceder au chateau.
  ]],
  [[
  je te laisse, je vais
  accueillir la
  nouvelle promo !
  ]],
  [[
  bon courage a toi !
  	
  ps: on dit ada pas 
  l'ada !
  ]])
end

function text_end()
 create_msg(" anne",
 [[
  oh coucou ada !
   
  tu as fait vite !
 ]],
 [[
  j'ai code un super 
  feux d'artifice
  rien que pour toi !
 ]],
 [[
  regardons ca ensemble
  et celebrons la venue
  de la nouvelle promo !
 ]],
 [[
  bon apres...petite
  partie de skyjo ?
  
  ou bien...
 ]],
 [[
          apero !!!!
          
  because that's all we need
 ]])
end
