function _init()
 p={
  x=13,y=56,
  f=false,
  sprite=20,
  key=0
 }
 gravity=1
 anne_text=true
 open=false
 init_msg()
 depart_msg()
end

function _update()
 move_perso()
 jumping()
 update_camera()
 dead_water()
 p_gravity()
 dead_hole()
 door_castle()
 update_msg()
end

function _draw()
 transp()
 map(0,0,0,0)
 grue()
 create_player()
 move_water()
 drowning()
 draw_msg()
 fireworks()
 elephant_water()
end

-->8
--personnage

function create_player()
 spr(p.sprite,p.x,p.y,1,1,p.f)
end

function update_camera()
 local map_width=(77*8)
 local map_height=(16*8)
 camx=mid(0,p.x-56,map_width-128)
 camy=mid(0,p.y-56,map_height)
 camera(camx,camy)
end

 
function move_p()
 if (btn(➡️) and p.x<600) then 
  p.x+=1
  p.f=false
 end
 if (btn(⬅️) and p.x>0) then
  p.x-=1
  p.f=true 
 end
 interact(p.x,p.y)
 unlock()
end

  
function jumping()
 if (btnp(⬆️)) and p.y==56 then 
  p.y-=17
  gravity=0.5
  sfx(3)
 end 
end

function move_perso()
 if not messages[1] then
 move_p()
 end
end

function p_gravity()
 if gravity>0 then 
  p.y+=gravity 
 end
end


